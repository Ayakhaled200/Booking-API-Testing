				{
					"name": "Get Booking Details",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {\r",
									"    pm.response.to.have.status(200);\r",
									"});\r",
									"\r",
									"//assert Json key value\r",
									"var jsonData = pm.response.json();\r",
									"pm.test(\"verify first name\", function () {\r",
									"    pm.expect(jsonData.firstname).to.eql(\"Testers\");\r",
									"});\r",
									"\r",
									"pm.test(\"verify total price\", function () {\r",
									"    pm.expect(jsonData.totalprice).to.eql(300);\r",
									"});\r",
									"\r",
									"//assert nested Json key value\r",
									"pm.test(\"verify checkin date\", function () {\r",
									"    pm.expect(jsonData.bookingdates.checkin).to.eql(\"2024-09-01\");\r",
									"});\r",
									"\r",
									"pm.test(\"verify checkout date\", function () {\r",
									"    pm.expect(jsonData.bookingdates.checkout).to.eql(\"2024-09-02\");\r",
									"});\r",
									"\r",
									"\r",
									"const expectedJSONschema = {\r",
									"  \"type\": \"object\",\r",
									"  \"properties\": {\r",
									"    \"firstname\": {\r",
									"      \"type\": \"string\"\r",
									"    },\r",
									"    \"lastname\": {\r",
									"      \"type\": \"string\"\r",
									"    },\r",
									"    \"totalprice\": {\r",
									"      \"type\": \"integer\"\r",
									"    },\r",
									"    \"depositpaid\": {\r",
									"      \"type\": \"boolean\"\r",
									"    },\r",
									"    \"bookingdates\": {\r",
									"      \"type\": \"object\",\r",
									"      \"properties\": {\r",
									"        \"checkin\": {\r",
									"          \"type\": \"string\"\r",
									"        },\r",
									"        \"checkout\": {\r",
									"          \"type\": \"string\"\r",
									"        }\r",
									"      },\r",
									"      \"required\": [\r",
									"        \"checkin\",\r",
									"        \"checkout\"\r",
									"      ]\r",
									"    }\r",
									"  },\r",
									"  \"required\": [\r",
									"    \"firstname\",\r",
									"    \"lastname\",\r",
									"    \"totalprice\",\r",
									"    \"depositpaid\",\r",
									"    \"bookingdates\"\r",
									"  ]\r",
									"};\r",
									"\r",
									"pm.test(\"verify JSON Schema\", function(){\r",
									"    pm.response.to.have.jsonSchema(expectedJSONschema)\r",
									"})\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
